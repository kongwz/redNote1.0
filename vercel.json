{
  "framework": "vite",
  "buildCommand": "npm run build",
  "installCommand": "rm -rf package-lock.json && npm install --legacy-peer-deps",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}